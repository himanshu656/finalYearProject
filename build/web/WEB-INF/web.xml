<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>com.Action.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewUserReg</servlet-name>
        <servlet-class>com.Action.NewUserReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CloudCreate</servlet-name>
        <servlet-class>com.Action.CloudCreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadCloud</servlet-name>
        <servlet-class>com.Action.UploadCloud</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLogin</servlet-name>
        <servlet-class>com.Action.UserLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadActions</servlet-name>
        <servlet-class>com.Action.UploadActions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileDownload</servlet-name>
        <servlet-class>com.Action.FileDownload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileDataFetcher</servlet-name>
        <servlet-class>com.Action.FileDataFetcher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertTags</servlet-name>
        <servlet-class>com.InsertTags</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileSearching</servlet-name>
        <servlet-class>com.Action.FileSearching</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetKey</servlet-name>
        <servlet-class>com.Action.GetKey</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DecryptFile</servlet-name>
        <servlet-class>com.Action.DecryptFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileOwner</servlet-name>
        <servlet-class>com.Action.FileOwner</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewUserReg</servlet-name>
        <url-pattern>/NewUserReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CloudCreate</servlet-name>
        <url-pattern>/CloudCreate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadCloud</servlet-name>
        <url-pattern>/UploadCloud</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLogin</servlet-name>
        <url-pattern>/UserLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadActions</servlet-name>
        <url-pattern>/UploadActions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileDownload</servlet-name>
        <url-pattern>/FileDownload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertTags</servlet-name>
        <url-pattern>/InsertTags</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileSearching</servlet-name>
        <url-pattern>/FileSearching</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileDataFetcher</servlet-name>
        <url-pattern>/FileDataFetcher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetKey</servlet-name>
        <url-pattern>/GetKey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DecryptFile</servlet-name>
        <url-pattern>/DecryptFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileOwner</servlet-name>
        <url-pattern>/FileOwner</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
